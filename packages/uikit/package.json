{
  "name": "availity-uikit",
  "description": "Availity UI Kit powered by Bootstrap 4",
  "version": "3.1.0",
  "author": "Robert McGuinness <rob.mcguinness@availity.com>",
  "style": "css/availity-uikit.css",
  "sass": "scss/availity-uikit.scss",
  "main": "index.js",
  "engines": {
    "node": ">= 8.0.0",
    "npm": "^4.0.0"
  },
  "scripts": {
    "build": "node-sass-chokidar --recursive --importer=node_modules/node-sass-tilde-importer scss --output css",
    "watch:scss": "node-sass-chokidar scss/ -o css/ --watch --recursive --importer=node_modules/node-sass-tilde-importer",
    "watch": "npm-run-all build watch:scss"
  },
  "license": "MIT",
  "homepage": "http://availity.github.io/availity-uikit/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Availity/availity-uikit.git"
  },
  "bugs": {
    "url": "https://github.com:Availity/availity-uikit/issues"
  },
  "files": [
    "images/",
    "fonts/",
    "scss/",
    "README.md"
  ],
  "contributors": [
    {
      "name": "Robert McGuinness",
      "email": "rob.mcguinness@availity.com"
    },
    {
      "name": "Robert Warner",
      "email": "rob.warner@availity.com"
    },
    {
      "name": "Bobby Bennett",
      "email": "bobby.bennett@availity.com"
    }
  ],
  "devDependencies": {
    "node-sass-chokidar": "^1.3.3",
    "node-sass-tilde-importer": "^1.0.2",
    "npm-run-all": "^4.1.3"
  },
  "keywords": [
    "css",
    "html",
    "bootstrap",
    "scss",
    "uikit",
    "framework",
    "web"
  ],
  "dependencies": {
    "bootstrap": "^4.0.0"
  }
}
