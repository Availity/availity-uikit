  "engines": {
    "node": ">= 5.0.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node dev/index.js -c start",
    "build:lib": "babel js --out-dir lib --quiet --ignore spec.js",
    "build:umd": "cross-env NODE_ENV=production node dev/index.js -c build",
    "build": "npm run build:umd && npm run build:lib",
    "docs": "cross-env NODE_ENV=development node dev/index.js -c docs",
    "clean": "node dev/index.js -c clean",
    "prepublish": "in-publish && npm run build || true",
    "release": "node dev/index.js -c release",
    "lint": "node dev/index.js -c lint"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "style": "dist/css/availity-uikit.css",
  "less": "lib/less/bootstrap.less",
  "homepage": "http://availity.github.io/availity-uikit/",
